{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "kafka-client",
  "version": "0.0.0",
  "identifier": "xyz.rumansaleem.kafka-client",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "decorations": false,
        "title": "Kafka Client",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  }
}